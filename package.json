{
  "name": "cory-project",
  "version": "1.0.0",
  "description": "Cory Authentication System - Full Stack Application",
  "private": true,
  "scripts": {
    "install:frontend": "cd cory && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "dev:frontend": "cd cory && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn app.api:app --reload --host 0.0.0.0 --port 8000",
    "build:frontend": "cd cory && npm run build",
    "lint:frontend": "cd cory && npm run lint",
    "lint:backend": "cd backend && pylint app/*.py --disable=W0718",
    "test:frontend": "cd cory && npm run test",
    "start:frontend": "cd cory && npm run preview",
    "start:backend": "cd backend && python -m uvicorn app.api:app --host 0.0.0.0 --port 8000"
  },
  "workspaces": [
    "cory"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cory-authentication.git"
  },
  "keywords": [
    "authentication",
    "react",
    "fastapi",
    "jwt",
    "oauth",
    "postgresql"
  ],
  "author": "Cory Development Team",
  "license": "MIT"
}
